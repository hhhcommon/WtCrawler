<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- 核心配置 -->
<configuration>
  <!-- 别名设置 -->
  <typeAliases>
    <typeAlias alias="source"      type="com.woting.crawler.core.scheme.persis.po.SourcePo"/>
    <typeAlias alias="scheme"      type="com.woting.crawler.core.scheme.persis.po.SchemePo"/>
    <typeAlias alias="batch"       type="com.woting.crawler.core.scheme.persis.po.CrawlBatchPo"/>
    <typeAlias alias="etl1Config"  type="com.woting.crawler.core.etl1.persis.po.Etl1ConfigPo"/>
    <typeAlias alias="etlLog"      type="com.woting.crawler.core.wtcm.persis.po.ResOrgAssetRefPo"/>
  </typeAliases>
  <!-- SQL文件设置 -->
  <mappers>
    <mapper resource="com/woting/crawler/core/scheme/persis/sql/Scheme.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/etl1/persis/sql/Etl1Config.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/wtcm/persis/sql/ResOrgAssetRef.mapper.xml"/>
    
  </mappers>
</configuration>